 Private Sub Deletes_Click()
 Call Clears
 For Each shts In ThisWorkbook.Worksheets 'Check if the worksheet is already existing
    If shts.Name = ActiveCell.Value Then
        Sheets(ActiveCell.Value).Delete
        Exit For
    End If
 Next shts
 ActiveCell.Value = "Meter " & ActiveCell.Row - 19
 End Sub

