Private Sub Import_Click()
 
Dim ExistS As Boolean
 
 '------------------------------------- Create Worksheet
 
 MeterName = ActiveWorkbook.Sheets("Main").Range(ActiveCell.Address).Value
 StatAdd = ActiveCell.Offset(, 2).Address()
 
 If Intersect(ActiveCell, Range("C20:C35")) Is Nothing Then
    MsgBox "Select the Meter Name to proceed. Try Again."
    Exit Sub
 Else
    If ActiveCell.Offset(, 1).Value = "" Then
       MsgBox "Select Meter Type first! Try Again"
       Exit Sub
    End If
 End If
 
 For Each sht In ThisWorkbook.Worksheets 'Check if the worksheet is already existing
    If sht.Name = MeterName Then
        ExistS = True
        Exit For
    End If
 Next sht
 
 If ExistS = False Then 'Create if not existing
    Sheets.Add(After:=Sheets(Sheets.Count)).Name = MeterName
 End If

 ActiveWorkbook.Sheets("Main").Activate

 '-------------------------------------- Calling Importing Procedures
 
 PQq = False
 
 Call ImpoW
 If Cont = False Then
    Exit Sub
 End If
 Call WPaster
 
 '---------------------------------- PQ Continuation
 
 If PQq = True Then
    Call PPaster
    PQq = False
    Cont = False
    Exit Sub
 End If
 
 '-----------------------------------PQ Logs Normal Sequence
 Call ImpoP
 If Cont = False Then
    Exit Sub
 End If
 Call PPaster
 
 
 PQq = False
 Cont = False
End Sub

